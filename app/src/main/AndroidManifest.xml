<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="videodemo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DaShenActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.MoveImageviewActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.MoveViewActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.WeigetMoveActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.WelcomeActivity"
            android:screenOrientation="portrait"></activity>

        <activity android:name="activity.RulerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.ViewpagerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="activity.GlideActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name="activity.RecycleViewActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="activity.video.ChoosePhotosActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name="activity.SViewOrRviewBattleActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>