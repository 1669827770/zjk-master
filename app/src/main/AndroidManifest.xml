<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="videodemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DaShenActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.MoveImageviewActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.MoveViewActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.WeigetMoveActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.WelcomeActivity"
            android:screenOrientation="portrait"></activity>

        <activity android:name="move.RulerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.ViewpagerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="move.GlideActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>




</manifest>